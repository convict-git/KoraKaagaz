<?xml version="1.0" encoding="UTF-8"?>
<!-- used when <testMode> value is set to true in resource/infrastructur_logger.xml --> 
<!-- this file is intended to be used by the test harness while running tests -->
<logging>
    <loggerOptions>
        <loggerOption LoggerName="FileLogger"   >false</loggerOption>
        <loggerOption LoggerName="ConsoleLogger">false</loggerOption>
    </loggerOptions>
    <logLevels>
        <logLevel level="ERROR"  >false</logLevel>
        <logLevel level="WARNING">false</logLevel>
        <logLevel level="SUCCESS">false</logLevel>
        <logLevel level="INFO"   >false</logLevel>
    </logLevels>
</logging>